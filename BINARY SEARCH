#include<stdio.h>
int main()
{
    int a[100];int size,key,i;int low,high,mid;
    printf("enter the size of array");
    scanf("%d",&size);
    
    for(i=0;i<size;i++)
    {
        scanf("%d",&a[i]);
    }
    printf("enter the key value you want to search");
    scanf("%d",&key);
    low=0;
    high=size-1;
    
    while(low<=high)
    {
        mid=(low+high)/2;
        if(key==a[mid])
        {
            printf("search elemnt found");
            break;
        }else if(key<a[mid])
        {
            high=mid-1;
        }else if(key>a[mid])
        {
            low=mid+1;
        }
    }
    
    if(low>high)
    {
        printf("search element not found");
    }
 
    
return 0;}
